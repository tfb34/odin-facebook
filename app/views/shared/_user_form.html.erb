<%= form_for @user do |f| %>
	<%=render 'shared/error_messages'%>
	
	Name: <%= f.text_field :name %><br>
	Email: <%= f.email_field :email%><br>
	Birthdate: 
	<%= f.date_select :birthdate, {order: [:month, :day, :year], start_year: Date.today.year, end_year: Date.today.year - 100}, {required: true} %>
	<%= f.submit %>

<% end %>